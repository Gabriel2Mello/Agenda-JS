<% if(errors.length > 0) { %>

<div class="row">
    <div class="col my-3">
        <div class="alert alert-danger">            
            <% errors.forEach(error => { %>
                <% console.log(error); %>
                <%= error %><br>
            <% }); %>
        </div>
    </div>
</div>

<% } %>

<% if(success.length > 0) { %>

    <div class="row">
        <div class="col my-3">
            <div class="alert alert-success">            
                <% success.forEach(message => { %>
                    <% console.log(message); %>
                    <%= message %><br>
                <% }); %>
            </div>
        </div>
    </div>
    
    <% } %>